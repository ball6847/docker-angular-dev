{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "overrides": [
    {
      "files": "*.component.html",
      "options": {
        "parser": "angular",
        "printWidth": 140,
        "htmlWhitespaceSensitivity": "ignore"
      }
    }
  ]
}
